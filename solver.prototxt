train_net: "train.prototxt"
test_net: "val.prototxt"
test_iter: 8000
# make test net, but don't invoke it from the solver itself
test_interval: 999999999
display: 40
average_loss: 40

# lr for unnormalized softmax
base_lr: 1e-12
lr_policy: "step"
gamma: 0.1
stepsize: 20000

# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 300000
weight_decay: 0.0002
snapshot: 2000
snapshot_prefix: "snapshot/inception_4s_city_batch_10classes"
test_initialization: false
